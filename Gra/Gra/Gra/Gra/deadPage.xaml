<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Gra.deadPage"
             xmlns:xct="http://xamarin.com/schemas/2020/toolkit"
             >
    <ContentPage.Content>
        <StackLayout BackgroundColor="Black">
            <xct:MediaElement x:Name="soundtrack" Source="https://www.soundhelix.com/examples/mp3/SoundHelix-Song-4.mp3"  IsLooping="True" IsVisible="false" ></xct:MediaElement>
            <Image x:Name="dead" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" Aspect="AspectFit" Source="dead"></Image>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>